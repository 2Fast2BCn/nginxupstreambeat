{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": {
          "enabled": false
        }
      },
      "dynamic_templates": [
        {
          "fields": {
            "mapping": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "match_mapping_type": "string",
            "path_match": "fields.*"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "name": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "nginx_upstream_status": {
		"properties": {
			"servers": {
				"properties": {
					"generation": {
						"type": "long"
					},
					"server": {
						"properties": {
							"fall": {
								"type": "long"
							},
							"index": {
								"type": "long"
							},
							"name": {
								"ignore_above": 1024,
								"index": "not_analyzed",
								"type": "string"
							},
							"port": {
								"type": "long"
							},
							"rise": {
								"type": "long"
							},
							"status": {
								"ignore_above": 1024,
								"index": "not_analyzed",
								"type": "string"
							},
							"type": {
								"ignore_above": 1024,
								"index": "not_analyzed",
								"type": "string"
							},
							"upstream": {
								"ignore_above": 1024,
								"index": "not_analyzed",
								"type": "string"
							}
						}
					},
					"total": {
						"type": "long"
					}
				}
			}
		}
        }
    }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "nginxupstreambeat-*"
}
